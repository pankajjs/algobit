generator client {
  provider = "prisma-client-js"
  output = "../../../node_modules/@prisma/client/web"

}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id            String      @id @default(auto()) @map("_id") @db.ObjectId
  email         String      @unique
  name          String
  createdAt     DateTime    @default(now())
  updatedAt     DateTime    @updatedAt
}